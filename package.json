{
  "name": "node-express-sequelize",
  "version": "0.0.1",
  "description": "node.js express sequelize typescript startup project",
  "main": "dist/server.js",
  "author": "Ivan Maga≈° aka wooglie",
  "license": "MIT",
  "scripts": {
    "archive": "git archive -v -o deploys/0.0.1.zip --format=zip HEAD",
    "build": "tsc",
    "bootstrap": "./tools/bootstrap.sh",
    "env": "dotenvi -s ",
    "start": "node dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript, Node\" -c \"yello.bold, cyan.bold\" \"yarn run watch-ts\" \"yarn run watch-node\"",
    "debug": "DEBUG=socket.io* yarn run watch",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/server.js",
    "db:create": "node scripts/create-database.js",
    "db:create:migration": "node scripts/create-migration-file.js",
    "db:drop": "node scripts/drop-database.js",
    "db:migrate": "node scripts/migrate-database.js migrate",
    "db:migrate:undo": "node scripts/migrate-database.js migrate:undo",
    "db:seed": "node scripts/seed-database.js seed:all",
    "db:seed:undo": "node scripts/seed-database.js seed:undo:all",
    "db:scrub": "yarn dotenvi -s \"$ENV\" && node scripts/db-scrub.js"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/joi": "^14.3.4",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/lodash": "^4.14.149",
    "@types/socket.io": "^2.1.4",
    "@types/uuid": "^3.4.6",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "dotenvi": "^0.6.0",
    "express": "^4.17.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mysql2": "^2.0.0",
    "nodemon": "^1.19.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^5.5.1",
    "sequelize-typescript": "^1.0.0",
    "shortid": "^2.2.15",
    "socket.io": "^2.3.0",
    "typescript": "^3.7.2",
    "uuid": "^3.3.3"
  },
  "dependencies": {
    "@types/shortid": "^0.0.29"
  }
}
